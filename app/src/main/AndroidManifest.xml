<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roy.clicksend" >
    <user-pramission android:name="android.permission.BLUETOOTH"/>
    <user-pramission android:name="android.permission.BLUETOOTH_ADMIN"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClickAndSend"
            android:label="@string/title_activity_click_and_send" >
        </activity>
        <activity
            android:name=".EditProfile"
            android:label="@string/title_activity_edit_profile" >
        </activity>
        <activity
            android:name=".NewProfile"
            android:label="@string/title_activity_new_profile" >
        </activity>
        <activity
            android:name=".HistoryList"
            android:label="@string/title_activity_history_list" >
        </activity>
    </application>

</manifest>
